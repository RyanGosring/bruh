(rule
 (target bar)
 (action
  (progn
   (echo "Building bar!\n")
   (with-stdout-to %{target} (echo "Hello from bar!")))))

(rule
 (target foo)
 (action
  (progn
   (echo "SHOULD NOT BE PRINTED!\n")
   (with-stdout-to %{target} (echo "Hello from foo!")))))

(rule
 (target foo_or_bar)
 (action (with-stdout-to %{target} (echo "bar"))))

(alias
 (name runtest)
 (action (dynamic-run ./client.exe)))
