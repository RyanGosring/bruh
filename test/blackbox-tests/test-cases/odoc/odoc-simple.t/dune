(library
 (name foo)
 (modules foo foo2 foo3)
 (wrapped false)
 (private_modules foo3)
 (public_name foo))

(library
 (name bar)
 (public_name bar)
 (modules bar))

(library
 (name foo_byte)
 (modules foo_byte)
 (modes byte)
 (public_name foo.byte))

(alias
 (name runtest)
 (deps _doc/_html/index.html)
 (action (echo "HTML:\n%{read:_doc/_html/index.html}\n\n")))
 
 (alias
  (name runtest)
  (deps _doc/_latex/index.tex)
  (action (echo "LATEX:\n%{read:_doc/_latex/index.tex}\n\n")))

(alias
 (name foo-mld)
 (deps _doc/_mlds/foo/index.mld)
 (action (echo "%{read:_doc/_mlds/foo/index.mld}")))

(alias
 (name bar-mld)
 (deps _doc/_mlds/bar/index.mld)
 (action (echo "%{read:_doc/_mlds/bar/index.mld}")))
