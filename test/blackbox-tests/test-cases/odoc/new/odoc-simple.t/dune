(library
 (name foo)
 (modules foo foo2 foo3)
 (wrapped false)
 (private_modules foo3)
 (public_name foo))

(library
 (name bar)
 (public_name bar)
 (modules bar))

(library
 (name foo_byte)
 (modules foo_byte)
 (modes byte)
 (public_name foo.byte))

